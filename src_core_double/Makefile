clean:
	(cd scalar/vsip_cscalar_d; make clean)
	(cd scalar/vsip_CSCALARm_d; make clean)
	(cd support/block/vsip_block_d; make clean) 
	(cd support/block/vsip_cblock_d; make clean) 
	(cd support/copy; make clean) 
	(cd support/matrix/vsip_cmview_d; make clean) 
	(cd support/matrix/vsip_mview_d; make clean) 
	(cd support/vector/vsip_cvview_d; make clean) 
	(cd support/vector/vsip_vview_d; make clean) 
	(cd vector/vsip_vview_d; make clean)
