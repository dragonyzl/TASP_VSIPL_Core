CC=gcc
INCLUDE=-I../include -I../../include/privateC
DEFINEP=-DVSIP_PERFORMANCE_MODE
DEFINED=-DVSIP_DEVELOPMENT_MODE
OPTIONS=-O2 -Wall -ansi -pedantic -fopenmp
AR=ar
AR_OPTIONS=rcvs
ARLIBP=libvsip_c.a
ARLIBD=libDvsip_c.a
COMPILEP=$(CC) $(DEFINEP) $(OPTIONS)
COMPILED=$(CC) $(DEFINED) $(OPTIONS)
performance:
	(cd scalar/vsip_cscalar_d; make COMPILE='$(COMPILEP)' AR='$(AR)' AR_OPTIONS='$(AR_OPTIONS)' ARLIB='$(ARLIBP)' obj)
	(cd scalar/vsip_CSCALARm_d; make COMPILE='$(COMPILEP)' AR='$(AR)' AR_OPTIONS='$(AR_OPTIONS)' ARLIB='$(ARLIBP)' obj)
	(cd support/block/vsip_block_d; make COMPILE='$(COMPILEP)' AR='$(AR)' AR_OPTIONS='$(AR_OPTIONS)' ARLIB='$(ARLIBP)' obj)
	(cd support/block/vsip_cblock_d; make COMPILE='$(COMPILEP)' AR='$(AR)' AR_OPTIONS='$(AR_OPTIONS)' ARLIB='$(ARLIBP)' obj)
	(cd support/copy; make COMPILE='$(COMPILEP)' AR='$(AR)' AR_OPTIONS='$(AR_OPTIONS)' ARLIB='$(ARLIBP)' obj)
	(cd support/matrix/vsip_cmview_d; make COMPILE='$(COMPILEP)' AR='$(AR)' AR_OPTIONS='$(AR_OPTIONS)' ARLIB='$(ARLIBP)' obj)
